<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dp" />
            <stroke android:width="1dip" android:color="#8625FB" />
            <gradient android:angle="-90" android:startColor="#8625FB" android:endColor="#536FE8"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#536FE8" />
            <solid android:color="#536FE8"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#3556E6" />
            <gradient android:angle="-90" android:startColor="#536FE8" android:endColor="#3556E6" />
        </shape>
    </item>
</selector>
